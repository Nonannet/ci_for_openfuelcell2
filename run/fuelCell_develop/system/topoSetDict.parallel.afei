        }
    }

    {
        name    electrolyte;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        sourceInfo
        {
            set electrolyte;
        }
    }

    {
        name    interconnect;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        sourceInfo
        {
            set interconnect;
        }
    }
