    pressureInletAir
    {
        type            surfaceFieldValue;
        libs            ("libfieldFunctionObjects.so");
        log             true;
        writeControl    timeStep;
        writeFields     no;
        surfaceFormat   none;
        regionType      patch;
        name            airInlet;
        region 		    air;	
        operation       areaAverage;
        fields
        (
            p
        );
    }
    
        pressureOutletAir
    {
        type            surfaceFieldValue;
