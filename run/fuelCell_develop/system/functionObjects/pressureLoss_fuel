    pressureInletFuel
    {
        type            surfaceFieldValue;
        libs            ("libfieldFunctionObjects.so");
        log             true;
        writeControl    timeStep;
        writeFields     no;
        surfaceFormat   none;
        regionType      patch;
        name            fuelInlet;
        region 		    fuel;	
        operation       areaAverage;
        fields
        (
            p
        );
    }
    
