     residualsAir
    {  
      type            solverInfo;
      libs            ("libutilityFunctionObjects.so");
      writeResidualFields no;

      writeControl    timeStep;
      region 	      air;

      fields (p_rgh e.water H2O.air O2.air);
    }  
    
         residualsFuel
    {  
      type            solverInfo;
      libs            ("libutilityFunctionObjects.so");
      writeResidualFields no;

      writeControl    timeStep;
      region 	      fuel;

      fields (p_rgh H2.fuel);
    }

    residualsPhiI
    {  
      type            solverInfo;
      libs            ("libutilityFunctionObjects.so");
      writeResidualFields no;

      writeControl    timeStep;
      region 	      phiI;

      fields (phi lambda);
    }
