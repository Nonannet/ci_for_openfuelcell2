FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.fuel 1
);

regions
(
    boxToCell
    {
        box (-40 -12.0 -0.25e-3) (40 12.0 0);
        fieldValues
        (
            volScalarFieldValue alpha.fuel 0.9
        );
    }
);


