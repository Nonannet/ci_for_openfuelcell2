FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1/air";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 313.15;

boundaryField
{
    airInlet
    {
        type            fixedValue;
        value           uniform 313.15;
    }
    airOutlet
    {
        type            zeroGradient;
    }
    cathodeSides
    {
        type            zeroGradient;
    }
    air_to_electrolyte
    {
        type            calculated;
        value           uniform 0;
    }
    air_to_interconnect
    {
        type            calculated;
        value           uniform 0;
    }
}


