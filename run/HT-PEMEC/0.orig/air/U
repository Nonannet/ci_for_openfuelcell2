FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/air";
    object      U.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.5 0 0);

boundaryField
{
    airInlet
    {
        type            fixedValue;
        value           uniform (0.5 0 0);
    }
    airOutlet
    {
        type            inletOutlet;
        phi             phi;
        inletValue      uniform (0 0 0);
        value           $internalField;
    }
    cathodeSides
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    air_to_electrolyte
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    air_to_interconnect
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
