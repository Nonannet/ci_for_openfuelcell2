FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     sofcFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1e3;

deltaT          1;

writeControl    runTime;

writeInterval   2e2;

purgeWrite      0;

writeFormat     ascii;

writePrecision  7;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

