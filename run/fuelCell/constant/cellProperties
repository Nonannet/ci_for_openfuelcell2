FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          cellProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dissolved
{
    type        standard;

    standardCoeffs
    {
        region      phiI; //- region name where the dissolved water is applied
        nd          0.12; //- 2.5/22
        rhoOnEW     1964; //- Density devided by equivalent weight

        ksi
        (
            acl 1.3       //- Anode adsorb/desorb rate
            ccl 1.3
        );

        relax 5.0e-4;     //- Relaxation factor
    }
}

galvanostatic
{
    active      true;     //- Galvanostatic true or not
    region      phiEC;    //- region where the galvanostatic control is applied
    patchName   interconnect0; //- patch name to control e.g. mean current density

    ibar 8000;                 //- Mean current density
}

nx    24;    //- Manual decomposition in x and y directions
ny    2;

