FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          yH2O;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.0;

boundaryField
{
    fuelInlet
    {
        type            fixedValue;
        value           $internalField;
    }

    fuelOutlet
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      uniform 0;
    }

    fuelSides
    {
        type            zeroGradient;
    }

    fuelInletSides
    {
        type            zeroGradient;
    }

    fuelOutletSides
    {
        type            zeroGradient;
    }

    anodeSides
    {
        type            zeroGradient;
    }

    aflSides
    {
        type            zeroGradient;
    }
    
    fuel_to_interconnect
    {
        type            zeroGradient;
    }

    fuel_to_electrolyte //anode
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
}


