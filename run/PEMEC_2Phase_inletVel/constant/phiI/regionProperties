FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          cellProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//- Add information here

control          false;
// relax           -2.0e-3;            // Hm is it enough, since I do not relax the phiEqn?
relax             -4.0e-11;

dissolveOnOff   true;

dissolved
{
    type        standard;

    standardCoeffs
    {
        region      phiI;
        nd          0.12;
        rhoOnEW     1964;

        ksi
        (
            acl 1.3
            ccl 1.3
        );

        relax 1.0e-4;
    }
}


sigma
{
    ccl
    {
        sigmaModel          lambdaSigma;

        lambdaSigmaCoeffs
        {
            cellZone            ccl;
            catalyst            yes;

            porosity            0.3;
            porNaf              0.3;
            lambdaName          lambda;
        }
    }

    acl
    {
        sigmaModel          lambdaSigma;

        lambdaSigmaCoeffs
        {
            cellZone            acl;
            catalyst            yes;

            porosity            0.3;
            porNaf              0.3;
            lambdaName          lambda;
        }
    }

    membrane
    {
        sigmaModel          lambdaSigma;
        lambdaSigmaCoeffs
        {
            cellZone            membrane;
            lambdaName          lambda;
        }
    }
}
