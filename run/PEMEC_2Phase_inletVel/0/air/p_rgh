FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";
    
    class           volScalarField;
    object          p;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{

    airInlet
    {
        type               fixedFluxPressure;
        value              $internalField;
    }

    airOutlet
    {
        type               prghTotalPressure;
        p                  $internalField;
        p0                 $internalField;
        value              $internalField;
        U                  U.air;
    }

    airSides
    {
        type               fixedFluxPressure;
        value              $internalField;
    }

    airInletSides
    {
        type               fixedFluxPressure;
        value              $internalField;
    }

    airOutletSides
    {
        type               fixedFluxPressure;
        value              $internalField;
    }

    cathodeSides
    {
        type               fixedFluxPressure;
        value              $internalField;
    }

    cflSides
    {
        type               fixedFluxPressure;
        value              $internalField;
    }

    air_to_interconnect
    {
        type               fixedFluxPressure;
        value              $internalField;
    }

    air_to_electrolyte //cathode
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
}


