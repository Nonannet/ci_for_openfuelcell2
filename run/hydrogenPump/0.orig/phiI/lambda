FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          phi;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.5;

boundaryField
{

	phiI_to_phiE1
    {
        type            zeroGradient;
		gradient			uniform -100;
    }
    
    phiI_to_phiE0
    {
        type            zeroGradient;
		gradient		uniform -100;
    }
    
    electrolyteSides
    {
        type            zeroGradient;
    }
    
	cathodeSides
    {
        type            zeroGradient;
    }
    
	anodeSides
    {
        type            zeroGradient;
    }

}


