FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          cellProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//- Add information here

active          false;
dissolveOnOff   true;
relax           -1.0e-3;

sigma
{
    ccl
    {
        sigmaModel          lambdaSigma;

        lambdaSigmaCoeffs
        {
            cellZone            ccl;
            catalyst            yes;

            porosity            0.3;
            porNaf              0.3;
            lambdaName          lambda;
        }
    }

    acl
    {
        sigmaModel          lambdaSigma;

        lambdaSigmaCoeffs
        {
            cellZone            acl;
            catalyst            yes;

            porosity            0.3;
            porNaf              0.3;
            lambdaName          lambda;
        }
    }

    membrane
    {
        sigmaModel          lambdaSigma;
        lambdaSigmaCoeffs
        {
            cellZone            membrane;
            lambdaName          lambda;
        }
    }
}
