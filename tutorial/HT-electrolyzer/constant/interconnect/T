FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1/interconnect";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 443.15;

boundaryField
{
    interconnect0
    {
        type            fixedValue;
        value           uniform 443.15;
    }
    interconnectSides
    {
        type            zeroGradient;
    }
    interconnect1
    {
        type            fixedValue;
        value           uniform 443.15;
    }
    interconnect_to_air
    {
        type            calculated;
        value           uniform 0;
    }
    interconnect_to_fuel
    {
        type            calculated;
        value           uniform 0;
    }
}


