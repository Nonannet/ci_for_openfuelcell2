FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1/air";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 1;

boundaryField
{
    airInlet
    {
        type            fixedValue;
        value           uniform 1e-15;
    }
    airInletSides
    {
        type            zeroGradient;
    }
    cathodeSides
    {
        type            zeroGradient;
    }
    airOutlet
    {
        type            zeroGradient;
    }
    airOutletSides
    {
        type            zeroGradient;
    }
    air_to_electrolyte
    {
        type            calculated;
        value           uniform 0;
    }
    air_to_interconnect
    {
        type            calculated;
        value           uniform 0;
    }
}


