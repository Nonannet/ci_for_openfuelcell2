FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1/phiEA";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 3;

boundaryField
{
    interconnectSides
    {
        type            zeroGradient;
    }
    interconnect1
    {
        type            zeroGradient;
    }
    anodeSides
    {
        type            zeroGradient;
    }
    phiEA_to_phiI0
    {
        type            calculated;
        value           uniform 0;
    }
    phiEA_to_phi0
    {
        type            calculated;
        value           uniform 0;
    }
}


