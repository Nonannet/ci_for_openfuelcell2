FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          regions;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//                                  fluid regions                            //
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

air
{
    name            air;
    regionType      fluid;
}

fuel
{
    name            fuel;
    regionType      fluid;
}


// ************************************************************************* //
//                          solid regions                                    //
// ************************************************************************* //


interconnect
{
    name            interconnect;
    regionType      solid;
}

electrolyte
{
    name            electrolyte;
    regionType      solid;
}


// ************************************************************************* //
//                          electron & ion regions                           //
// ************************************************************************* //

phiEA
{
    name            phiEA;
    regionType      electronIon;

    sigma
    {
        phiEA
        {
            sigmaModel      constantSigma;

            constantSigmaCoeffs
            {
                cellZone    phiEA;
                sigma       20000;
            }
        }

        acl
        {
            sigmaModel      porousSigma;

            porousSigmaCoeffs
            {
                cellZone        acl;
                catalyst        yes;
                porosity        0.0;
                sigma           5000;

                porNaf          1.0;
            }
        }

        anode
        {
            sigmaModel      porousSigma;

            porousSigmaCoeffs
            {
                cellZone        anode;
                catalyst        no;
                porosity        0.0;
                sigma           8000;
            }
        }
    }
}

phiEC
{
    name            phiEC;
    regionType      electronIon;
    relax           1.0e-5;
    sigma
    {
        phiEC
        {
            sigmaModel      constantSigma;

            constantSigmaCoeffs
            {
                cellZone        phiEC;
                sigma           20000;
            }
        }

        ccl
        {
            sigmaModel      porousSigma;

            porousSigmaCoeffs
            {
                cellZone        ccl;
                catalyst        yes;
                porosity        0.0;
                sigma           5000;

                porNaf          1.0;
            }
        }

        cathode
        {
            sigmaModel      porousSigma;

            porousSigmaCoeffs
            {
                cellZone        cathode;
                catalyst        no;
                porosity        0.0;
                sigma           8000;
            }
        }
    }
}

phiI
{
    name            phiI;
    regionType      electronIon;
    active          yes;

    relax           -2.0e-3;

    sigma
    {
        ccl
        {
            sigmaModel          lambdaSigma;

            lambdaSigmaCoeffs
            {
                cellZone            ccl;
                catalyst            yes;

                porosity            0.14;
                porNaf              0.39;
                lambdaName          dissolved;
            }
        }

        acl
        {
            sigmaModel          lambdaSigma;

            lambdaSigmaCoeffs
            {
                cellZone            acl;
                catalyst            yes;

                porosity            0.14;
                porNaf              0.39;
                lambdaName          dissolved;
            }
        }

        membrane
        {
            sigmaModel          lambdaSigma;
            lambdaSigmaCoeffs
            {
                cellZone            membrane;
                lambdaName          dissolved;
            }
        }
    }
}
