FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          cellProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//- Add information here

active          false;

dissolveOnOff   true;
relax           -4.0e-3;

sigma
{
    ccl
    {
        sigmaModel          constantSigma;

        constantSigmaCoeffs
        {
            cellZone            ccl;
            sigma               5;
        }
    }

    acl
    {
        sigmaModel          constantSigma;

        constantSigmaCoeffs
        {
            cellZone            ccl;
            sigma               5;
        }
    }

    membrane
    {
        sigmaModel          constantSigma;

        constantSigmaCoeffs
        {
            cellZone            ccl;
            sigma               5;
        }
    }
}
