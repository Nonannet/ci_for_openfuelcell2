FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1/phiEA";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];


internalField   uniform 353.15;

boundaryField
{
    interconnectSides
    {
        type            zeroGradient;
    }
    anodeSides
    {
        type            zeroGradient;
    }
    interconnect1
    {
        type            fixedValue;
        value           uniform 353.15;
    }
    phiEA_to_phi0
    {
        type            calculated;
        value           uniform 0;
    }
    phiEA_to_phiI0
    {
        type            calculated;
        value           uniform 0;
    }
}


