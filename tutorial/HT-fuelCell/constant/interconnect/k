FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1/interconnect";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];


internalField   uniform 1;

boundaryField
{
    interconnect0
    {
        type            zeroGradient;
    }
    interconnectSides
    {
        type            zeroGradient;
    }
    interconnect1
    {
        type            zeroGradient;
    }
    interconnect_to_air
    {
        type            calculated;
        value           uniform 0;
    }
    interconnect_to_fuel
    {
        type            calculated;
        value           uniform 0;
    }
}


